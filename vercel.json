{
  "version": 2,
  "functions": {
    "/index.js": {
      "maxDuration": 30
    },
    "/paynet.js": {
      "maxDuration": 20
    }
  },
  "builds": [
    {
      "src": "/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/ping",
      "dest": "/index.js"
    },
    {
      "src": "/paynet/(.*)",
      "dest": "/paynet.js"
    },
    {
      "src": "/webhook/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "crons": [
    {
      "path": "/ping",
      "schedule": "*/5 * * * *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
